# .env.example
# Environment variables for LangChain Serverless Application

# AWS Configuration
AWS_REGION=us-east-1
AWS_PROFILE=default

# Deployment Configuration
STAGE=dev
SERVICE_NAME=langchain-text-transformer

# API Configuration
API_TIMEOUT=30
API_MEMORY_SIZE=512

# Monitoring
ENABLE_XRAY=true
LOG_LEVEL=info

# Copy this file to .env and fill in your values
# cp .env.example .env
